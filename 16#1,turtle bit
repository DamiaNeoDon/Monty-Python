from turtle import Turtle, Screen


timmy =Turtle()

print(timmy)
timmy.shape("turtle")
timmy.color("DeepPink1")
timmy.fd(100)



my_screen =Screen()
print(my_screen.canvheight)
my_screen.exitonclick()
